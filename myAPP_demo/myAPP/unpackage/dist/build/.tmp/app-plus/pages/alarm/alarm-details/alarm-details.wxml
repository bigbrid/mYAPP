<view class="container"><view class="alarm-details" style="border-top:none;"><view class="alarm-name">{{"项目名称："+project_name}}</view><view class="alarm-value"><view class="alarm-row">当前PM<text>{{project_now}}</text></view><view class="alarm-row">标站值<text>{{project_station_num}}</text></view></view><view class="alarm-bottom"><view class="icon-position icon"></view><text class="alarm-bottom_text" style="flex:1;">{{project_address}}</text><view class="icon-position icon"></view><text class="alarm-bottom_text" style="width:174rpx;">{{"承建方:"+project_constructor}}</text></view></view><view class="alarm-details" style="padding-top:0;"><view class="c-title"><view class="c-title_left">报警记录</view><view data-event-opts="{{[['tap',[['onAlarmAll',['$event']]]]]}}" class="c-title_right" bindtap="__e"><text>全部</text><view class="icon-left_arrow"></view></view></view></view><view class="alarm-details_table"><uni-table vue-id="1" tbody="{{list}}" thead="{{thead}}" rank="{{0}}" bind:__l="__l"></uni-table></view><view class="alarm-details" style="padding-top:10rpx;"><uni-joint vue-id="2" list="{{module}}" data-event-opts="{{[['^switchChange',[['switchChange']]]]}}" bind:switchChange="__e" bind:__l="__l"></uni-joint></view></view>