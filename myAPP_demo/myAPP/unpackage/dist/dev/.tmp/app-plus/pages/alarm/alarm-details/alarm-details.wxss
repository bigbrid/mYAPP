
.alarm-details{
	position: relative;
	z-index: 9;
	padding: 26rpx 33rpx 0;
	background-color: #fff;
	border-top: 20rpx  #F6F6F6 solid;
}
.alarm-name{
	font-size:32rpx;
	line-height:45rpx;
	color:rgba(51,51,51,1);
}
.alarm-value{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 24rpx;
	color: #333333;
	margin-top: 13rpx;
	border-bottom: 1px solid rgba(238,238,238,1);
	padding-bottom: 17rpx;
}
.alarm-row{
	margin-right: 37rpx;
}
.alarm-row text{font-size: 24rpx;color: #FC511F;}
.alarm-bottom{
	height: 72rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.alarm-bottom .icon-position{
	background: url(data:image/png;base64,) no-repeat;
	width: 20rpx;
	height: 24rpx;
	background-size: 100% 100%;
}
.alarm-bottom .alarm-bottom_text{
	font-size:24rpx;
	line-height:33rpx;
	color:rgba(153,153,153,1);
	padding: 0 10rpx;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
}
.alarm-details_table{
	background-color: #fff;
	padding-bottom: 40rpx;
	margin-top: -35rpx;
}



