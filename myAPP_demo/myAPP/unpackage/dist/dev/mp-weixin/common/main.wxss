button::after {
	border: none;
}
page {
	font-family: PingFang SC;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	min-height: 100%;
}
text,
view {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	line-height: 1;
}
.container {
	min-height: calc(100vh - 44px);
}
.flex-direction_column {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
}
.c-title {
	height: 102rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.c-title_left {
	height: 45rpx;
	font-size: 32rpx;
	line-height: 45rpx;
	color: rgba(51, 51, 51, 1);
}
.c-title_right {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	font-size: 28rpx;
	line-height: 40rpx;
	color: rgba(153, 153, 153, 1);
}
.icon-left_arrow {
	background: url(data:image/png;base64,) no-repeat;
	width: 14rpx;
	height: 28rpx;
	background-size: 100% 100%;
	margin-left: 13rpx;
}
.from-view {
	width: 100vw;
}
.from-view .leabe-row {
	width: 100%;
	padding: 0 33rpx;
	height: 102rpx;
	border-top: 1px solid #e6e6e6;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	background-color: #fff;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.from-view .leabe-row .input {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	margin-left: 10rpx;
	height: 44rpx;
	line-height: 44rpx;
}
.from-type2 .leabe-row .input {
	margin-left: 20rpx;
}
.icon-view {
	width: 40rpx;
	height: 100%;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
}
.icon-key {
	width: 34rpx;
	height: 34rpx;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
}
.icon-clear {
	width: 36rpx;
	height: 37rpx;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
	border-radius: 50%;
}
.icon-arts {
	width: 32rpx;
	height: 38rpx;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
}
.scroll-wrap {
	width: 100%;
	margin-top: 20rpx;
	height: calc(100vh - 88rpx);
}
.scroll-content {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	width: 750rpx;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}

.table-defalut_text {
	color: #333;
	font-size: 22rpx;
	padding: 5rpx;
	text-align: center;
}
.font-green {
	font-size: 22rpx;
	color: #09ac57 !important;
}
.font-orange {
	font-size: 22rpx;
	color: #fc511f !important;
}
.font-blue {
	font-size: 22rpx;
	color: #2999f8 !important;
}

